  <ObjectType SymbolicName="Prefix7:ControllerType">
    <Description>Configuration and operation of the zone controller</Description>
    <Children>
      <Variable SymbolicName="Prefix7:ActualOutput" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Indicates the currently active output in percent</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
      <Property SymbolicName="Prefix7:ActualType" ModellingRule="Mandatory" DataType="Prefix7:ControllerTypeEnumeration">
        <Description>Actual value for the controller type used by the zone</Description>
        <DefaultValue>
          <uax:Int32>0</uax:Int32>
        </DefaultValue>
      </Property>
      <Property SymbolicName="Prefix7:ActualValueActive" ModellingRule="Mandatory" DataType="Prefix1:Boolean">
        <Description>Indicates the current status of the controller</Description>
        <DefaultValue>
          <uax:Boolean>false</uax:Boolean>
        </DefaultValue>
      </Property>
      <Property SymbolicName="Prefix7:AutomaticReferenceZoneSelection" ModellingRule="Optional" DataType="Prefix1:Boolean" AccessLevel="ReadWrite">
        <Description>If true, the HRD selects automatically the reference zone.</Description>
      </Property>
      <Variable SymbolicName="Prefix7:AverageControllerOutput" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Indicates the average output which can be used when a sensor is broken</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
      <Object SymbolicName="Prefix7:LoadCurrent" TypeDefinition="Prefix5:MonitoredParameterType" ModellingRule="Optional">
        <Description>Information about the load current in Ampere</Description>
      </Object>
      <Object SymbolicName="Prefix7:LoadPower" TypeDefinition="Prefix5:MonitoredParameterType" ModellingRule="Optional">
        <Description>Information about the load power in Watt</Description>
      </Object>
      <Variable SymbolicName="Prefix7:LowerOutput" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" AccessLevel="ReadWrite">
        <Description>Limitation of the minimum output in closed-loop control in %</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
      <Property SymbolicName="Prefix7:OutputTime" ModellingRule="Optional" DataType="Prefix1:Duration" AccessLevel="ReadWrite">
        <Description>Time basis for operating the actuator</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Property>
      <Property SymbolicName="Prefix7:ReferenceZone" ModellingRule="Optional" DataType="Prefix1:UInt32" AccessLevel="ReadWrite">
        <Description>If zones are to operate parallel to a control zone, the reference relation can be realised with this parameter.</Description>
        <DefaultValue>
          <uax:UInt32>0</uax:UInt32>
        </DefaultValue>
      </Property>
      <Property SymbolicName="Prefix7:SetValueActive" ModellingRule="Mandatory" DataType="Prefix1:Boolean" AccessLevel="ReadWrite">
        <Description>A control zone is switched on and off with this parameter.</Description>
      </Property>
      <Variable SymbolicName="Prefix7:SetValueManualOutput" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" AccessLevel="ReadWrite">
        <Description>Manually given output in percent if SetValueType = MANUAL is selected</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
      <Variable SymbolicName="Prefix7:SetValueType" TypeDefinition="Prefix1:MultiStateValueDiscreteType" ModellingRule="Mandatory" DataType="Prefix1:UInt16" AccessLevel="ReadWrite">
        <Description>Set value for the controller type used by the zone</Description>
        <Children>
          <Property SymbolicName="Prefix1:EnumValues" DataType="Prefix1:EnumValueType" ValueRank="Array" />
        </Children>
      </Variable>
      <Variable SymbolicName="Prefix7:UpperOutput" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" AccessLevel="ReadWrite">
        <Description>Limitation of the maximum output in closed-loop control in %</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
      <Variable SymbolicName="Prefix7:UpperSetValueCascade" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" AccessLevel="ReadWrite">
        <Description>If the two controllers are cascaded, the master defines to which nominal temperature value the slave is to control to. With this parameter, the upper limit for the slave is defined.</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
    </Children>
    <ClassName />
  </ObjectType>
  <ObjectType SymbolicName="Prefix7:HeatUpType">
    <Description>Alternative to the SetRamp functionality of MonitoredParameterType defined in OPC 40083. With HeatUpType, it can be pre-defined how the control circuit is to be operated when it is switched on for the next time; with or without heat-up process.</Description>
    <Children>
      <Property SymbolicName="Prefix7:EvenHeatUpEnabled" ModellingRule="Optional" DataType="Prefix1:Boolean" AccessLevel="ReadWrite">
        <Description>Enables even heat-up process until nominal SetValue of Temperature of ZoneType is reached</Description>
      </Property>
      <Property SymbolicName="Prefix7:ManualOutputLimitActive" ModellingRule="Optional" DataType="Prefix1:Boolean" AccessLevel="ReadWrite">
        <Description>Activates heat-up process with pre-defined SetValueManualOutputLimit until SetValueTemperature is reached</Description>
      </Property>
      <Property SymbolicName="Prefix7:RelayHeatingGroup" ModellingRule="Optional" DataType="Prefix1:Byte" AccessLevel="ReadWrite">
        <Description>Number of the heating group for relay heating</Description>
      </Property>
      <Property SymbolicName="Prefix7:RelayHeatingTime" ModellingRule="Optional" DataType="Prefix1:Duration" AccessLevel="ReadWrite">
        <Description>Time for relay heating of the zone. When RelayHeatingTime of all zones of a heating group have expired, the next group starts heating.</Description>
      </Property>
      <Variable SymbolicName="Prefix7:SetValueManualOutputLimit" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" AccessLevel="ReadWrite">
        <Description>This pre-defined maximum output (in percent) is valid until the SetValueTemperature is reached.</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
      <Variable SymbolicName="Prefix7:SetValueTemperature" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" AccessLevel="ReadWrite">
        <Description>If ManualOutputLimitActive is set, SetValueManualOutputLimit is active until this nominal temperature value is reached.</Description>
        <DefaultValue>
          <uax:Double>0</uax:Double>
        </DefaultValue>
      </Variable>
    </Children>
    <ClassName />
  </ObjectType>
