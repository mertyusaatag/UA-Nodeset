  <ObjectType SymbolicName="Prefix7:LDSCycleParametersEventType" BaseType="Prefix1:BaseEventType" IsAbstract="true">
    <Description>Information on a dosing cycle</Description>
    <Children>
      <Variable SymbolicName="Prefix7:AdditivesPressure" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" ValueRank="Array" ArrayDimensions="0">
        <Description>Average pressure of the additive during the last cycle at the measuring point</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:AdditivesRatioActual" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" ValueRank="Array" ArrayDimensions="0">
        <Description>Actual ratios of additive in percentage</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:AdditivesRatioTarget" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" ValueRank="Array" ArrayDimensions="0">
        <Description>Target ratios of additives in percentage which are set in AdditiveFraction of AdditiveType</Description>
      </Variable>
      <Property SymbolicName="Prefix7:CycleNumber" ModellingRule="Mandatory" DataType="Prefix1:UInt64">
        <Description>Number of the dosing cycle</Description>
      </Property>
      <Property SymbolicName="Prefix7:DosingTime" ModellingRule="Optional" DataType="Prefix1:Duration">
        <Description>Duration of the dosing cycle</Description>
      </Property>
      <Variable SymbolicName="Prefix7:FilterPressurePrimary" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Average material pressure during the last cycle before the filter</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:FilterPressureSecondary" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Average material pressure during the last cycle after the filter</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:MixingPointPressureA" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Average pressure of component A during the last cycle at the blender</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:MixingPointPressureB" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Average pressure of component B during the last cycle at the blender</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:MixingPointPressureBlender" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Average pressure of components A&amp;B during the last cycle at the blender</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:MixingRatioActual" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Actual mixing ratio of the components</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:MixingRatioTarget" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Target mixing ratio of the last cycle (includes ratio change when MaterialBalanceSystem is active)</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:ResidualAmountA" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Residual weight amount of component A at the end of the dosing cycle</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:ResidualAmountB" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Residual weight amount of component B at the end of the dosing cycle</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:VolumeA" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Volume of component A that was added to the process in the last cycle</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:VolumeAB" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Volume of components A + B that was added to the process in the last cycle</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:VolumeAdditives" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double" ValueRank="Array" ArrayDimensions="0">
        <Description>Volumes of the additives that were added to the process in the last cycle</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:VolumeB" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Volume of component B that was added to the process in the last cycle</Description>
      </Variable>
      <Variable SymbolicName="Prefix7:VolumeTotal" TypeDefinition="Prefix1:AnalogItemType" ModellingRule="Optional" DataType="Prefix1:Double">
        <Description>Volume of all components (A + B + all additives)</Description>
      </Variable>
    </Children>
    <ClassName />
  </ObjectType>
